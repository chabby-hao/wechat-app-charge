/* pages/register/register.wxss */
page{
  height: 100%;
}

.register {
  height: 100%;
  padding: 146rpx 70rpx 0 70rpx;
  text-align: left;
}

.page-section{
  width:100%;
}

.wxinput_view{
  position: relative;
  /* width: 100%; */
  margin-bottom: 24rpx;
}

.wxinput{
  padding-left: 30rpx;
  border: 2rpx solid white;
  border-radius: 40rpx;
  color:#FEFEFE;
}

.wxinput-top{
  width: 380rpx;
}

.wxplace{
  /* margin-left:30rpx; */
  font-size: 30rpx;
  color: #FEFEFE;
  opacity: 0.6;
}

.wxinput_bt{
  border-radius: 0rpx;
  font-size: 30rpx;
  position: absolute;
  right:0;
  top:0;
  height: 84rpx;
  width:240rpx;
  background-color: transparent;
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
  }

.wxbt_view{
  margin-top: 32rpx;
}

.wxbt{
  background-color: transparent;
  border:none;
  font-size: 30rpx;
  width:100%;
  /* height:100%; */
  background-size: 100% 100%;
  background-image: url("data:image/png;base64,");
}
