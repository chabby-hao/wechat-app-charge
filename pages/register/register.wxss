/* pages/register/register.wxss */

.register {
  padding: 146rpx 62rpx 0 62rpx;
  text-align: left;
}

.page-section {
  width: 100%;
  margin-bottom: 0;
}

.wxinput_view {
  position: relative;
}

.wxinput {
  padding-left: 30rpx;
  border: 2rpx solid white;
  border-radius: 40rpx;
  color: #fefefe;
}

.wxplace {
  /* margin-left:30rpx; */
  font-size: 30rpx;
  color: #fefefe;
  opacity: 0.6;
}

.wxinput_bt {
  background: url("data:image/png;base64,");
  background-size: 100% 100%;
  z-index: 999;
  border-radius: 0rpx;
  font-size: 26rpx;
  position: absolute;
  right: 0;
  top: 0;
  height: 84rpx;
  line-height: 84rpx;
  width: 214rpx;
  padding: 2rpx 0;
  border: none;
  background-color: transparent;
  color: #307f8e;
  text-align: center;
  vertical-align: middle;
  padding-left: 16rpx;
}

.img_yanzheng {
  width: 214rpx;
  height: 80rpx;
}

button::after {
  content: none;
}

.wxbt_view {
  text-align: center;
  width: 100%;
}

.wxbt {
  text-align: center;
  background-color: transparent;
  border: none;
  padding: 0;
}

.img_register {
  width: 626rpx;
  height: 96rpx;
}

.input-margin {
  margin-bottom: 24rpx;
}

.bt-margin {
  margin-top: 54rpx;
}

.agree-view{
  text-align: center;
  color:#fefefe;
  font-size: 26rpx;
}
